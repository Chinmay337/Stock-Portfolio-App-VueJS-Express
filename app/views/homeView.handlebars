<h3>Home Page for Dummy Stock Portfolio App</h3>

<div id="inp" style="border: 1px"></div>
<div id="result" > </div>

<script>

 function loadFunction() {

const search = location.search;
 console.log(search);
     const urlParams = new URLSearchParams(search);

let result = '';
for (const entry of urlParams.entries()) {
    console.log(entry);
    result = result + '<li>' + entry[0] + ' = ' + entry[1] + '</li>'
}

if(result === ''){
    result = "No parameters found";
}
else{
    result = '<ul>' + result + '</ul>';
}



document.getElementById('inp').innerHTML = search;
document.getElementById('result').innerHTML = result;
 }
   window.onload = loadFunction();
</script>
